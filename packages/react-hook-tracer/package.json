{
  "name": "react-hook-tracer",
  "author": "Martijn Schrage (https://www.oblomov.com)",
  "version": "0.2.0",
  "description": "Light-weight tracer for React hooks",
  "license": "MIT",
  "keywords": [
    "react",
    "hooks",
    "tracing",
    "debugging",
    "spy"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oblosys/react-hook-tracer.git",
    "directory": "packages/react-hook-tracer"
  },
  "homepage": "https://github.com/Oblosys/react-hook-tracer#readme",
  "bugs": {
    "url": "https://github.com/Oblosys/react-hook-tracer/issues"
  },
  "files": [
    "dist/*",
    "!dist/*.test.*",
    "src/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "cp src/components/*.css dist/components",
    "build-watch": "yarn --silent build --watch",
    "clean": "rm -rf dist && rm react-hook-tracer-*.tgz",
    "prepack": "yarn --silent test && yarn --silent build",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
