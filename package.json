{
  "name": "react-hook-tracer-demo",
  "author": "Martijn Schrage (https://www.oblomov.com)",
  "version": "1.0.0",
  "private": true,
  "description": "Demo for the react-hook-tracer package",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "cd packages/react-hook-tracer && yarn build && cp src/components/*.css dist/components",
    "build-watch": "cd packages/react-hook-tracer && yarn build-watch",
    "clean-lib": "cd packages/react-hook-tracer && yarn clean",
    "eject": "react-scripts eject",
    "lint-check": "eslint --cache .",
    "lint-fix": "yarn lint-check -- --fix",
    "pack-lib": "cd packages/react-hook-tracer && npm pack",
    "prettier-check": "prettier -l '**/*.{ts,tsx,json,md,js,scss,yml}'",
    "prettier-fix": "yarn prettier-check -- --write",
    "publish-lib": "yarn build && cd packages/react-hook-tracer && npm publish",
    "test": "react-scripts test",
    "test-lib": "cd packages/react-hook-tracer && yarn test",
    "test-lib-watch": "cd packages/react-hook-tracer && yarn test-watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-tracer": "0.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "customize-cra": "^1.0.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "27.0.4",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "27.5.1",
    "prettier": "^2.7.1",
    "ts-jest": "27.1.5"
  }
}
